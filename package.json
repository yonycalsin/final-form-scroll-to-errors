{
  "name": "final-form-scroll-to-errors",
  "version": "0.1.0",
  "keywords": [
    "final-form-focus",
    "form-focus",
    "final-form-scroll-to-errors",
    "scroll-into-view-if-needed",
    "react-form-scroll"
  ],
  "homepage": "https://github.com/yonycalsin/final-form-scroll-to-errors#readme",
  "bugs": {
    "url": "https://github.com/yonycalsin/final-form-scroll-to-errors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonycalsin/final-form-scroll-to-errors.git"
  },
  "license": "MIT",
  "author": "Yony Calsin",
  "main": "dist/index.js",
  "module": "dist/final-form-scroll-to-errors.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "build-storybook": "build-storybook",
    "format": "npm run prettier -- --write",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@size-limit/preset-small-lib": "^7.0.4",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@yonycalsin/prettier-config-react-typescript": "^1.0.1",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "semantic-release": "^18.0.1",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/final-form-scroll-to-errors.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/final-form-scroll-to-errors.esm.js",
      "limit": "10 KB"
    }
  ]
}
